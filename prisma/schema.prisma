generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Author {
  id        Int    @id @default(sequence())
  FirstName String
  LastName  String
  books     Book[]
}

model Book {
  id          Int      @id @default(sequence())
  Title       String
  Description String
  authors     Author[]
}
