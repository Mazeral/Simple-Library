generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Author {
  id  Int @id @default(autoincrement())
  FirstName String 
  LastName String
  books Book[]
}

model Book{
  id Int @id @default(autoincrement())
  Title String 
  Description String
  authors Author[]
}